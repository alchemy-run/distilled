{
  "name": "distilled-code",
  "version": "0.0.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "distilled": "./src/index.ts"
  },
  "scripts": {
    "distilled": "bun run src/index.ts",
    "tui": "bun run src/index.ts ./examples/distilled.config.ts",
    "cli": "bun run src/index.ts ./examples/distilled.config.ts -p",
    "list-agents": "bun run src/index.ts ./examples/distilled.config.ts -l",
    "test": "vitest run",
    "test:watch": "vitest",
    "download:env": "doppler secrets download --no-file --format env > .env"
  },
  "devDependencies": {
    "@effect/vitest": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@effect/ai": "catalog:",
    "@effect/ai-anthropic": "catalog:",
    "@effect/ai-openai": "catalog:",
    "@effect/cli": "catalog:",
    "@effect/experimental": "catalog:",
    "@effect/platform": "catalog:",
    "@effect/platform-node": "catalog:",
    "@opentui/core": "catalog:",
    "@opentui/solid": "catalog:",
    "diff": "catalog:",
    "effect": "catalog:",
    "solid-js": "^1.9.10",
    "web-tree-sitter": "catalog:"
  }
}