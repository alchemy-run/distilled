{
  "name": "distilled-coinbase",
  "version": "0.0.1",
  "files": [
    "lib",
    "src"
  ],
  "type": "module",
  "sideEffects": false,
  "module": "src/index.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "bun": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./Category": {
      "types": "./lib/category.d.ts",
      "bun": "./src/category.ts",
      "default": "./lib/category.js"
    },
    "./Client": {
      "types": "./lib/client.d.ts",
      "bun": "./src/client.ts",
      "default": "./lib/client.js"
    },
    "./Credentials": {
      "types": "./lib/credentials.d.ts",
      "bun": "./src/credentials.ts",
      "default": "./lib/credentials.js"
    },
    "./Errors": {
      "types": "./lib/errors.d.ts",
      "bun": "./src/errors.ts",
      "default": "./lib/errors.js"
    },
    "./Operations": {
      "types": "./lib/operations/index.d.ts",
      "bun": "./src/operations/index.ts",
      "default": "./lib/operations/index.js"
    },
    "./Pagination": {
      "types": "./lib/pagination.d.ts",
      "bun": "./src/pagination.ts",
      "default": "./lib/pagination.js"
    },
    "./Retry": {
      "types": "./lib/retry.d.ts",
      "bun": "./src/retry.ts",
      "default": "./lib/retry.js"
    },
    "./Sensitive": {
      "types": "./lib/sensitive.d.ts",
      "bun": "./src/sensitive.ts",
      "default": "./lib/sensitive.js"
    },
    "./Traits": {
      "types": "./lib/traits.d.ts",
      "bun": "./src/traits.ts",
      "default": "./lib/traits.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate": "bun run scripts/generate-operations.ts",
    "build": "bun run generate && tsgo -b",
    "typecheck": "tsgo",
    "test": "bunx vitest run",
    "test:read": "bunx vitest run tests/read-only.test.ts",
    "test:mutating": "bunx vitest run tests/mutating.test.ts",
    "lint": "oxlint",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check ."
  },
  "dependencies": {},
  "devDependencies": {
    "@effect/platform": "^0.94.1",
    "@effect/platform-bun": "^0.87.0",
    "@effect/vitest": "^0.27.0",
    "@types/bun": "catalog:",
    "dotenv": "^17.2.3",
    "effect": "^3.19.14",
    "jose": "^6.0.11",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@effect/platform": "catalog:",
    "effect": "catalog:",
    "jose": ">=6.0.0"
  }
}
